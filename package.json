{
  "name": "wtfapp",
  "version": "0.0.1",
  "description": "WTF App",
  "author": "Pratheesh Thazhathidoth",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.7",
    "@nestjs/core": "^5.3.7",
    "@nestjs/jwt": "^0.1.3",
    "@nestjs/passport": "^1.1.0",
    "ajv": "^6.5.3",
    "cache-manager": "^2.9.0",
    "cheerio": "^1.0.0-rc.2",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "dotenv": "^6.0.0",
    "fastify-formbody": "^2.0.0",
    "helmet": "^3.13.0",
    "mongoose": "^5.2.16",
    "mongoose-autopopulate": "^0.8.1",
    "nest-schedule": "^0.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redis-typescript": "^0.1.2",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rxjs": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "5.3.2",
    "typescript": "^2.6.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.3.7",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/mongoose": "^5.2.16",
    "@types/node": "^9.6.31",
    "@types/supertest": "^2.0.6",
    "jest": "^29.3.1",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "supertest": "^3.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
